<?xml version="1.0" encoding="utf-8"?>
<resources>

<declare-styleable name="CustomSpinnerView">

	<attr name="textColor" format="integer"/>
	<attr name="textSize" format="integer"/>
	<attr name="textColor2" format="integer"/>
	<attr name="textSize2" format="integer"/>
	<attr name="itemsVisible" format="integer"/>
	<attr name="textAlign">
		<enum name="left" value="0" />
        <enum name="center" value="1" />
        <enum name="right" value="2" />
	</attr>
	<attr name="text2Align">
		<enum name="left" value="0" />
        <enum name="center" value="1" />
        <enum name="right" value="2" />
	</attr>
	<attr name="iconAlign">
		<enum name="left" value="0" />
        <enum name="center" value="1" />
        <enum name="right" value="2" />
	</attr>

</declare-styleable>

</resources>
